-#  This file is app/views/jsonfiles/index.html.haml
%h1 All Json Files

%table#jsonfiles
  %thead
    %tr
      %th= "Json File"
  %tbody
    - @jsonfiles.each do |file|
      %tr
        %td= file.name
        %td= link_to "More about #{file.name}", jsonfile_path(file)

= link_to 'Add new Json File', new_jsonfile_path

